<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn">
	<Appenders>
		<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout
				pattern="%d	%-5p Line:%L %-100.100c%nThread:[%t]%n	Message:%m%n%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Logger name="in.gauravbrills.springtdd" level="debug">
			<AppenderRef ref="STDOUT" />
		</Logger>
		<Root level="warn">
			<AppenderRef ref="STDOUT" />
		</Root>
	</Loggers>
</Configuration>